{
  "name": "calculator.solution",
  "version": "1.0.0",
  "description": "See how to configure Jest and Cypress with React, Babel, and Webpack",
  "main": "index.js",
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "test:debug:win": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "dev": "webpack-serve",
    "build": "webpack --mode=production",
    "precommit": "lint-staged && npm run test:e2e:run",
    "postbuild": "cp ./public/index.html ./dist/index.html",
    "start": "serve --no-clipboard --listen 8080 dist",
    "format": "prettier \"**/*.js\" --write",
    "validate": "npm run test:coverage && npm run test:e2e:run",
    "setup": "npm run setup && npm run validate"
  },
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "GPLv3",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.51",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-emotion": "^9.2.4",
    "css-loader": "^0.28.11",
    "cypress": "^3.0.1",
    "cypress-testing-library": "^2.2.0",
    "emotion": "^9.2.4",
    "eslint": "^5.0.1",
    "eslint-config-kentcdodds": "^14.0.0",
    "file-loader": "^1.1.11",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^1.1.1",
    "jest": "^23.1.0",
    "jest-dom": "^1.3.1",
    "jest-emotion": "^9.2.4",
    "jest-runner-eslint": "^0.6.0",
    "jest-watch-select-projects": "0.0.1",
    "jest-watch-typeahead": "^0.1.0",
    "lint-staged": "^7.2.0",
    "prop-types": "^15.6.2",
    "react-emotion": "^9.2.4",
    "react-testing-library": "^4.0.2",
    "serve": "^9.1.0",
    "start-server-and-test": "^1.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-point": "^3.0.1"
  }
}
