{
  "name": "calculator.solution",
  "version": "1.0.0",
  "description": "See how to configure Jest and Cypress with React, Babel, and Webpack",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "pretest:e2e": "npm run build",
    "test:e2e": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8080 cy:open",
    "dev": "webpack-serve",
    "build": "webpack --mode=production",
    "start": "serve --listen 8080",
    "validate": "npm run test",
    "setup": "npm run setup && npm run validate"
  },
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "GPLv3",
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "@babel/core": "^7.0.0-beta.51",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "css-loader": "^0.28.11",
    "cypress": "^3.0.1",
    "cypress-testing-library": "^2.2.0",
    "file-loader": "^1.1.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "serve": "^9.1.0",
    "start-server-and-test": "^1.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-serve": "^1.0.4"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-loadable": "^5.4.0",
    "react-point": "^3.0.1"
  }
}
